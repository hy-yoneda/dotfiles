[core]
	autocrlf = false
	editor = vim
[alias]
	st = status
	co = checkout
	df = diff
	ds = diff --staged
	ci = commit
	ci-amend = commit --amend
	ci-amend-no-edit = commit --amend --no-edit
	graph = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
